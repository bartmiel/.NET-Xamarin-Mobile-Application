<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:onboarding="clr-namespace:ProjektZaliczeniowy.Modules.Onboarding"
             xmlns:login="clr-namespace:ProjektZaliczeniowy.Modules.Login"
             xmlns:register="clr-namespace:ProjektZaliczeniowy.Modules.Register"
             x:Class="ProjektZaliczeniowy.LoginShell">
    
    <ShellContent Route="OnboardingViewModel"
                  ContentTemplate="{DataTemplate onboarding:OnboardingView}" />
    
    <ShellContent Route="LoginViewModel"
                  ContentTemplate="{DataTemplate login:LoginView}" />
    
    <ShellContent Route="RegisterViewModel"
                  ContentTemplate="{DataTemplate register:RegisterView}" />
    
</Shell>